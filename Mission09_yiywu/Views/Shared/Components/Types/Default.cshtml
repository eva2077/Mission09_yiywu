@{
}
@model IEnumerable<string>

<div class="d-grid gap-2">
    <a asp-controller="Home" asp-action="Index" asp-route-BookCategory="" asp-route-pageNum="1" class="btn btn-outline-secondary">Home</a>
<br />
@foreach (string t in Model)
{
        <a asp-asp-controller="Home" asp-action="Index" asp-route-BookCategory="@t" asp-route-pageNum="1"
            class="btn @(ViewBag.SelectedCategory == t? "btn-primary": "btn btn-outline-secondary")">@t</a>
        <br />
}
</div>

